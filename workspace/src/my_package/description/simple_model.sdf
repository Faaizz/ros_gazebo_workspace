    <!-- Read JointTrajectory message from /set_joint_trajectory topic and move the model around accordingly -->
    <!-- sample message: ros2 topic pub -1 /set_joint_trajectory trajectory_msgs/msg/JointTrajectory '{header: {frame_id: world}, joint_names: [slider_joint, arm_joint], points: [  {positions: {0.8,0.6}} ]}' -->
